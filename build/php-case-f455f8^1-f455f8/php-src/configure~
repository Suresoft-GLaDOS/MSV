# DB2
if test "$PHP_DB2" != "no"; then

  unset THIS_INCLUDE THIS_LIBS THIS_LFLAGS THIS_PREFIX THIS_RESULT

  if test "$HAVE_DB3" = "1" || test "$HAVE_DB4" = "1"; then

