rm -f core conftest.err conftest.$ac_objext conftest.beam \
    conftest$ac_exeext conftest.$ac_ext
    fi

    echo > ext/iconv/php_have_bsd_iconv.h
    echo > ext/iconv/php_have_glibc_iconv.h
    echo > ext/iconv/php_have_libiconv.h
    echo > ext/iconv/php_have_ibm_iconv.h

    case "$iconv_impl_name" in
      gnu_libiconv )
        PHP_DEFINE(PHP_ICONV_IMPL,\"libiconv\",ext/iconv)

printf "%s\n" "#define PHP_ICONV_IMPL \"libiconv\"" >>confdefs.h

        PHP_DEFINE(HAVE_LIBICONV,1,ext/iconv)
        PHP_ADD_LIBRARY_WITH_PATH(iconv, "$PHP_ICONV_PREFIX/$PHP_LIBDIR", ICONV_SHARED_LIBADD)
        ;;

      bsd )
        PHP_DEFINE(HAVE_BSD_ICONV,1,ext/iconv)

printf "%s\n" "#define HAVE_BSD_ICONV 1" >>confdefs.h

        PHP_DEFINE(PHP_ICONV_IMPL,\"BSD iconv\",ext/iconv)

printf "%s\n" "#define PHP_ICONV_IMPL \"BSD iconv\"" >>confdefs.h

        ;;

      glibc )
        PHP_DEFINE(HAVE_GLIBC_ICONV,1,ext/iconv)

printf "%s\n" "#define HAVE_GLIBC_ICONV 1" >>confdefs.h

        PHP_DEFINE(PHP_ICONV_IMPL,\"glibc\",ext/iconv)

printf "%s\n" "#define PHP_ICONV_IMPL \"glibc\"" >>confdefs.h

        ;;
      ibm )
        PHP_DEFINE(HAVE_IBM_ICONV,1,ext/iconv)

printf "%s\n" "#define HAVE_IBM_ICONV 1" >>confdefs.h

        PHP_DEFINE(PHP_ICONV_IMPL,\"IBM iconv\",ext/iconv)

printf "%s\n" "#define PHP_ICONV_IMPL \"IBM iconv\"" >>confdefs.h

        ;;
    esac

    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking if iconv supports errno" >&5
printf %s "checking if iconv supports errno... " >&6; }
    if test "$cross_compiling" = yes
then :

      { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: no" >&5
printf "%s\n" "no" >&6; }
      PHP_DEFINE(ICONV_SUPPORTS_ERRNO,0,ext/iconv)

printf "%s\n" "#define ICONV_SUPPORTS_ERRNO 0" >>confdefs.h


else $as_nop
  cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

#include <$PHP_ICONV_H_PATH>
#include <errno.h>

int main() {
  iconv_t cd;
  cd = iconv_open( "*blahblah*", "*blahblah*" );
  if (cd == (iconv_t)(-1)) {
    if (errno == EINVAL) {
      return 0;
	} else {
      return 1;
    }
  }
  iconv_close( cd );
  return 2;
}

_ACEOF
if ac_fn_c_try_run "$LINENO"
then :

      { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: yes" >&5
printf "%s\n" "yes" >&6; }
      PHP_DEFINE(ICONV_SUPPORTS_ERRNO,1,ext/iconv)

printf "%s\n" "#define ICONV_SUPPORTS_ERRNO 1" >>confdefs.h


else $as_nop

      { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: no" >&5
