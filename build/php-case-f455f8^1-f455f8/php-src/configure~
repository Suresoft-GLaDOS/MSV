  conftest.$ac_objext conftest.beam conftest.$ac_ext
fi


    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking if your cpp allows macro usage in include lines" >&5
printf %s "checking if your cpp allows macro usage in include lines... " >&6; }
    cat confdefs.h - <<_ACEOF >conftest.$ac_ext
