printf "%s\n" "no" >&6; }
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking whether /dev/arandom exists" >&5
printf %s "checking whether /dev/arandom exists... " >&6; }
  if test -r "/dev/arandom" && test -c "/dev/arandom"; then

printf "%s\n" "#define HAVE_DEV_ARANDOM 1" >>confdefs.h

