  if test "$ac_cv_crypt_md5" = "yes"; then
    ac_result=1
    ac_crypt_md5=1
  else
    ac_result=0
    ac_crypt_md5=0
  fi

printf "%s\n" "#define PHP_MD5_CRYPT $ac_result" >>confdefs.h


  if test "$ac_cv_crypt_sha512" = "yes"; then
    ac_result=1
    ac_crypt_sha512=1
  else
    ac_result=0
    ac_crypt_sha512=0
  fi

printf "%s\n" "#define PHP_SHA512_CRYPT $ac_result" >>confdefs.h


  if test "$ac_cv_crypt_sha256" = "yes"; then
    ac_result=1
    ac_crypt_sha256=1
  else
    ac_result=0
    ac_crypt_sha256=0
  fi

printf "%s\n" "#define PHP_SHA256_CRYPT $ac_result" >>confdefs.h



printf "%s\n" "#define PHP_USE_PHP_CRYPT_R 0" >>confdefs.h

fi

ac_fn_c_check_func "$LINENO" "getcwd" "ac_cv_func_getcwd"
