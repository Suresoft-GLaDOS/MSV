  --with-iodbc[=DIR]      Include iODBC support [/usr/local])

  if test "$PHP_IODBC" != "no"; then
    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for iODBC support" >&5
printf %s "checking for iODBC support... " >&6; }
    if test "$PHP_IODBC" = "yes"; then
