{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $ac_cv_type_fp_except" >&5
printf "%s\n" "$ac_cv_type_fp_except" >&6; }
  if test "$ac_cv_type_fp_except" = "yes"; then

