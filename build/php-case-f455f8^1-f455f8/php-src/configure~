{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $ac_cv_int_type_uint32_t" >&5
printf "%s\n" "$ac_cv_int_type_uint32_t" >&6; }
if test "$ac_cv_int_type_uint32_t" = "yes"; then

