printf "%s\n" "$ac_cv_int_type_int32_t" >&6; }
if test "$ac_cv_int_type_int32_t" = "yes"; then

printf "%s\n" "#define HAVE_INT32_T 1" >>confdefs.h

fi


{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for uint32_t" >&5
