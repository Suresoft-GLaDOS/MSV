  if test "$ac_cv_3arg_setrebindproc" = yes; then

printf "%s\n" "#define HAVE_3ARG_SETREBINDPROC 1" >>confdefs.h

  fi

      ac_fn_c_check_func "$LINENO" "ldap_parse_result" "ac_cv_func_ldap_parse_result"
